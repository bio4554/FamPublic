<?xml version="1.0" encoding="utf-8"?>
<android.support.constraint.ConstraintLayout
    xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:background="@color/colorPrimary"
    android:id="@+id/constraintLayout4">

    <ImageView
        android:src="@drawable/fam_logo"
        android:layout_width="283dp"
        android:layout_height="124dp"
        android:id="@+id/imageView3"
        app:layout_constraintLeft_toLeftOf="@+id/constraintLayout4"
        android:layout_marginStart="16dp"
        app:layout_constraintTop_toTopOf="@+id/constraintLayout4"
        android:layout_marginTop="16dp"
        app:layout_constraintRight_toRightOf="@+id/constraintLayout4"
        android:layout_marginEnd="16dp" />

    <TextView
        android:text="welcome to fam pre alpha test. bio4554"
        android:layout_width="298dp"
        android:layout_height="118dp"
        android:id="@+id/textViewGreet"
        android:textAlignment="center"
        android:textSize="18sp"
        app:layout_constraintLeft_toLeftOf="@+id/constraintLayout4"
        android:layout_marginStart="16dp"
        app:layout_constraintTop_toBottomOf="@+id/imageView3"
        android:layout_marginTop="8dp"
        app:layout_constraintRight_toRightOf="@+id/constraintLayout4"
        android:layout_marginEnd="16dp" />

    <TextView
        android:text="logout"
        android:layout_width="193dp"
        android:layout_height="46dp"
        android:id="@+id/textViewLogout"
        android:textAlignment="center"
        android:textSize="30sp"
        app:layout_constraintLeft_toLeftOf="@+id/constraintLayout4"
        android:layout_marginStart="16dp"
        app:layout_constraintTop_toBottomOf="@+id/textViewGreet"
        android:layout_marginTop="8dp"
        app:layout_constraintRight_toRightOf="@+id/constraintLayout4"
        android:layout_marginEnd="16dp" />

</android.support.constraint.ConstraintLayout>